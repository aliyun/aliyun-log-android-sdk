<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aliyun.sls.android.producer.example">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".SLSDemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">

        <activity
            android:name=".example.trace.TraceActivity"
            android:exported="true"
            android:label="@string/title_activity_trace" />
        <activity android:name=".example.trace.ui.FragmentActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SettingsActivity" />
        <activity android:name=".example.producer.ProducerExample" />
        <activity android:name=".example.producer.ProducerWithImmediately" />
        <activity android:name=".example.producer.ProducerWithNoPersistent" />
        <activity android:name=".example.producer.ProducerWithDynamicConfig" />
        <activity android:name=".example.producer.ProducerWithMultiClients" />
        <activity android:name=".example.producer.ProducerWithDestroy" />
        <activity android:name=".example.crash.CrashExampleActivity"/>
    </application>

</manifest>